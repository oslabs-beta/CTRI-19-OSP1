const express = require('express');
const addMetricRouter = express.Router();
const addMetricController = require('../controllers/addMetricController');

addMetricRouter.post('/pull', (req, res) => {
  res.status(200).write('hello');
});

addMetricRouter.get('/', (req, res) => {
  res.status(200).sendFile('src/components/pages/AddMetric/index.html');
});

module.exports = addMetricRouter;
